<div class="panel panel-default">
  <div class="panel-heading">Form history_timesheet</div>
  <div class="panel-body">
<%= form_for(@history_timesheet, html: { class: "form-horizontal", role: "form" }) do |f| %>
  <% if @history_timesheet.errors.any? %>
    <div class="alert alert-danger alert-dismissable" role="alert">
      <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
      <h4><%= pluralize(@history_timesheet.errors.count, "error") %> prohibited this history_timesheet from being saved:</h4>

      <ul>
      <% @history_timesheet.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :parent_timesheet_id, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :parent_timesheet_id, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :approve_status, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :approve_status, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :approve_by, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :approve_by, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :reject_by, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :reject_by, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <%= f.submit class: "btn btn-primary" %>
    </div>
  </div>
<% end %>
  </div>
</div>